layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Arash's Persian blog</title>
 <link href="http://fa.arashTaher.ir/atom.xml" rel="self"/>
 <link href="http://fa.arashTaher.ir"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://fa.arashTaher.ir/</id>
 <author>
   <name>Arash Taher</name>
   <!-- <email></email> -->
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://fa.arashTaher.ir{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://fa.arashTaher.ir{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
